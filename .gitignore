# WordPress .gitignore file
# Source: https://salferrarello.com/wordpress-gitignore/

# Ignore everything in wp-content except for themes and plugins directories
wp-content/*
!wp-content/themes/
!wp-content/plugins/
wp-content/themes/*
!wp-content/themes/twentytwentyfive/
!wp-content/themes/twentytwentyfour/
!wp-content/themes/twentytwentythree/
!wp-content/themes/mtq-aceh-pidie-jaya/
wp-content/plugins/*
!wp-content/plugins/akismet/
!wp-content/plugins/hello.php

# Ignore WordPress core updates
wp-admin/
wp-includes/
/*.php

# Project specific ignores
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Theme specific ignores
wp-content/themes/mtq-aceh-pidie-jaya/node_modules/
wp-content/themes/mtq-aceh-pidie-jaya/.sass-cache/
wp-content/themes/mtq-aceh-pidie-jaya/dist/
wp-content/themes/mtq-aceh-pidie-jaya/.npm/

# Temporary and debug files
*.log
*.tmp
*.temp
*-debug.php
*-fix.php
debug-*.php
test-*.php
prototype-backup-*.tar.gz

# IDE files
.vscode/
.idea/
*.sublime-*

# Build and dependency files
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
/*.html
/*.txt
wp-*.php
xmlrpc.php
wp-config.php
wp-content/uploads/
wp-content/upgrade/
wp-content/cache/

# Ignore log files and database
*.log
*.sql
*.sqlite

# Ignore OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# Ignore node_modules and build artifacts
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore composer files
vendor/
composer.phar
composer.lock

# Ignore backup files
*~
.bak
.backup
.save

# Ignore environment files
.env
.env.local
.env.production
.env.staging

# Ignore WordPress config sample if not needed
wp-config-sample.php