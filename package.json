{
    "name": "mtq-aceh-pidie-jaya-theme",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "dev": "tailwindcss -c tailwind.config.js -i ./wp-content/themes/mtq-aceh-pidie-jaya/assets/css/app.css -o ./wp-content/themes/mtq-aceh-pidie-jaya/dist/app.css --watch",
        "build": "NODE_ENV=production tailwindcss -c tailwind.config.js -i ./wp-content/themes/mtq-aceh-pidie-jaya/assets/css/app.css -o ./wp-content/themes/mtq-aceh-pidie-jaya/dist/app.css --minify",
        "zip:theme": "bash ./scripts/release-theme.sh zip",
        "release:theme": "bash ./scripts/release-theme.sh release",
        "release:dry": "bash scripts/release-theme.sh 1.0.1 --dry-run --no-release --no-tag",
        "release": "bash scripts/release-theme.sh"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.16",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.41",
        "tailwindcss": "^3.4.10"
    }
}
